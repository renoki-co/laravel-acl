{
    "name": "renoki-co/laravel-acl",
    "description": "Simple, AWS IAM-style ACL for Laravel applications, leveraging granular permissions in your applications with strong declarations.",
    "keywords": ["laravel", "php"],
    "license": "Apache-2.0",
    "homepage": "https://github.com/renoki-co/laravel-acl",
    "authors": [
        {
            "name": "Alex Renoki",
            "homepage": "https://github.com/rennokki",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.35",
        "renoki-co/acl": "^0.4.0"
    },
    "autoload": {
        "psr-4": {
            "RenokiCo\\LaravelAcl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RenokiCo\\LaravelAcl\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.1",
        "orchestra/database": "^7.0",
        "orchestra/testbench": "^7.0",
        "orchestra/testbench-core": "^7.0",
        "phpunit/phpunit": "^9.6.4",
        "vimeo/psalm": "4.24.0"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "RenokiCo\\LaravelAcl\\LaravelAclServiceProvider"
            ]
        }
    }
}
